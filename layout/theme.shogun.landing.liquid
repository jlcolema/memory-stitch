    </script>
    <!-- Bing - Passing Sales Value -->
    <script async>
        window.uetq = window.uetq || [];
        window.uetq.push({
            'revenue_value': {{ total_price | money_without_currency }},
            'currency': '{{ shop.currency }}'
        });
    </script>
	<!-- End of Bing -->


  {% render 'shogun-head' %}
</head>


<body class="template-{{ template | split: '.' | first }}">


  {{ content_for_layout }}
  {{ 'instant.js' | asset_url | script_tag }}
  <style type="text/css">
    body, html {
      width: 100% !important;
      background: white !important;
    }
  </style>
  {{ content_for_header }}
</body>

</html>

